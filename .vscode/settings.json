{
  "files.associations": {
    "*.css": "postcss",
    "*.overlay": "dts",
    "*.keymap": "dts",
    "sensor.h": "c",
    "stdint.h": "c",
    "compare": "c",
    "array": "c",
    "string_view": "c",
    "initializer_list": "c",
    "string": "c",
    "numeric": "c",
    "event_manager.h": "c",
    "behavior.h": "c",
    "hid.h": "c",
    "input-event-codes.h": "c",
    "device.h": "c"
  },
  "[c]": {
    "editor.formatOnSave": true
  },
  "[javascript][javascriptreact][typescript][typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "[css][json][jsonc][html][markdown][yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
